{
  "name": "vue-vvc-player",
  "version": "1.0.0",
  "description": "Vue 3 H.266/VVC 视频播放器组件",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./sdk/*": "./sdk/*"
  },
  "bin": {
    "vvc-sdk-install": "./scripts/copy-sdk.js"
  },
  "files": [
    "dist",
    "sdk",
    "src",
    "scripts"
  ],
  "scripts": {
    "build": "vite build",
    "prepublishOnly": "npm run build",
    "sdk:install": "node scripts/copy-sdk.js"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "vue": "^3.5.16",
    "vite": "^6.3.5",
    "@vitejs/plugin-vue": "^5.2.4",
    "typescript": "^5.8.3"
  },
  "keywords": [
    "vue",
    "vue3",
    "h266",
    "vvc",
    "video",
    "player",
    "wasm"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  }
}
